{
  "name": "x-project-backend",
  "version": "1.0.0",
  "private": false,
  "description": "Backend for x project",
  "license": "MIT",
  "author": "Muhammad Afif, DKK",
  "scripts": {
    "build": "nest build",
    "commit": "cz",
    "db:dev": "dotenv -e .env.development -- npm run migrate:dev && npm run seed:dev",
    "db:prod": "dotenv -e .env.production -- npm run migrate:prod && npm run seed:prod",
    "docker:down:dev": "dotenv -e .env.development -- docker compose down",
    "docker:down:prod": "dotenv -e .env.production -- docker compose down",
    "docker:up:dev": "dotenv -e .env.development -- docker compose up -d",
    "docker:up:prod": "dotenv -e .env.production -- docker compose up -d",
    "find-deadcode": "ts-prune -p tsconfig.json -e",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "generate": "npx prisma generate",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --max-warnings=0",
    "migrate": "npx prisma migrate dev --schema ./prisma/schema.prisma",
    "migrate:dev": "dotenv -e .env.development -- npx prisma migrate dev --schema ./prisma/schema.prisma",
    "migrate:prod": "dotenv -e .env.production -- npx prisma migrate dev --schema ./prisma/schema.prisma",
    "prepare": "husky install",
    "seed": "npx ts-node -r tsconfig-paths/register ./prisma/seeder/seeder.ts",
    "seed:dev": "dotenv -e .env.development -- npx ts-node -r tsconfig-paths/register ./prisma/seeder/seeder.ts",
    "seed:prod": "dotenv -e .env.production -- npx ts-node -r tsconfig-paths/register ./prisma/seeder/seeder.ts",
    "sort:package": "sort-package-json",
    "start": "nest start",
    "start:debug": "dotenv -e .env.development -- nest start --debug --watch",
    "start:dev": "dotenv -e .env.development -- nest start --watch",
    "start:prod": "dotenv -e .env.production -- node dist/src/main",
    "studio": "npx prisma studio",
    "studio:dev": "dotenv -e .env.development -- npx prisma studio",
    "studio:prod": "dotenv -e .env.production -- npx prisma studio",
    "typecheck": "tsc --noEmit"
  },
  "husky": {
    "hooks": {
      "prepare-commit-msg": "exec < /dev/tty && git cz --hook || true"
    }
  },
  "lint-staged": {
    "package.json": "pnpm sort:package",
    "src/**/*.ts": [
      "pnpm format"
    ]
  },
  "dependencies": {
    "@fastify/static": "^7.0.0",
    "@keyv/redis": "^4.3.3",
    "@nestjs/axios": "^3.1.3",
    "@nestjs/bull": "^10.2.2",
    "@nestjs/bullmq": "^10.2.2",
    "@nestjs/cache-manager": "^2.3.0",
    "@nestjs/common": "^10.4.6",
    "@nestjs/config": "^3.3.0",
    "@nestjs/core": "^10.4.6",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/passport": "^10.0.3",
    "@nestjs/platform-express": "^10.4.6",
    "@nestjs/platform-fastify": "^10.4.6",
    "@nestjs/serve-static": "^4.0.2",
    "@nestjs/swagger": "^8.0.5",
    "@prisma/client": "^5.22.0",
    "@twitter-api-v2/plugin-token-refresher": "^1.0.0",
    "axios": "^1.9.0",
    "bcrypt": "^5.1.1",
    "bull": "^4.16.4",
    "bullmq": "^5.23.0",
    "cache-manager": "^5.2.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "date-fns": "^4.1.0",
    "dotenv": "^16.4.5",
    "express": "^4.21.1",
    "express-session": "^1.18.1",
    "keyv": "^5.3.3",
    "nestjs-file-upload": "^1.0.4",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "reflect-metadata": "^0.2.2",
    "rimraf": "^5.0.1",
    "rxjs": "^7.8.1",
    "sanitize-html": "^2.13.1",
    "twitter-api-v2": "^1.22.0",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@darraghor/eslint-plugin-nestjs-typed": "4.6.1",
    "@nestjs/cli": "^10.4.5",
    "@nestjs/schematics": "^10.2.3",
    "@nestjs/testing": "^10.4.6",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@types/bcrypt": "^5.0.2",
    "@types/cache-manager": "^4.0.6",
    "@types/express": "^5.0.0",
    "@types/express-session": "^1.18.1",
    "@types/jest": "29.5.14",
    "@types/multer": "^1.4.12",
    "@types/node": "^22.8.6",
    "@types/objects-to-csv": "^1.3.3",
    "@types/passport-jwt": "^4.0.1",
    "@types/passport-local": "^1.0.38",
    "@types/sanitize-html": "^2.13.0",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-import-resolver-typescript": "^3.6.3",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-import-helpers": "^1.3.1",
    "eslint-plugin-jest": "^28.8.3",
    "eslint-plugin-jest-formatting": "^3.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-unicorn": "^56.0.0",
    "husky": "^9.1.6",
    "jest": "29.7.0",
    "lint-staged": "^15.2.10",
    "prettier": "^3.3.3",
    "prisma": "^5.22.0",
    "semantic-release": "^24.2.0",
    "sort-package-json": "^2.10.1",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-jest": "29.2.5",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "ts-prune": "^0.10.3",
    "tsconfig-paths": "4.2.0",
    "typescript": "5.3.3",
    "webpack": "^5.96.1"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}